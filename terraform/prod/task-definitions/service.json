[
  {
    "name": "ppv-api",
    "mountPoints": [
      {
        "sourceVolume": "ppv-app",
        "containerPath": "/var/www/html/ppv"
      }
    ],
    "image": "780172389235.dkr.ecr.eu-west-1.amazonaws.com/ppv-prod:latest",
    "hostname": "ppv-api-prod",
    "cpu": 200,
    "memory": 100,
    "essential": true,
    "logConfiguration": {
      "logDriver": "fluentd",
      "options": {
        "fluentd-address": "logs.prod.mettalloids.com:24224",
        "tag": "docker.ppv.prod"
      }
    },
    "portMappings": [
      {
        "containerPort": 9000,
        "hostPort": 9000
      }
    ]
  }
]